{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "type": "shell",
      "command": "pnpm",
      "args": [
        "run",
        "build"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$tsc"
      ]
    },
    {
      "label": "dev",
      "type": "shell",
      "command": "pnpm",
      "args": [
        "run",
        "start:dev"
      ],
      "group": "none",
      "presentation": {
        "reveal": "always",
        "panel": "new"
      },
      "problemMatcher": []
    },
    {
      "label": "test",
      "type": "shell",
      "command": "pnpm",
      "args": [
        "run",
        "test"
      ],
      "group": {
        "kind": "test",
        "isDefault": true
      },
      "problemMatcher": [
        "$jest"
      ]
    },
    {
      "label": "test:e2e",
      "type": "shell",
      "command": "pnpm",
      "args": [
        "run",
        "test:e2e"
      ],
      "group": "test",
      "problemMatcher": [
        "$jest"
      ]
    }
  ]
}
